#include <unistd.h>

void ft_putchar(char c)
{
	write(1, &c, 1);

}


void ft_putstr(char *str)
{
	int i;

	i = 0;
	while(str[i] != '\0')
	{
		ft_putchar(str[i]);
		i++;
	}
}


int main(int argc, char **argv)
{
	
	int i; 
	int n;

	i = 1;
	n = argc;
	while(i < argc)
	{
		ft_putstr(argv[i]);
		ft_putchar('\n');	
		i++;
	}

}
